<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vothuong.anhdemo1">

    <application
        android:name=".AnhApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListViewActivity" />
        <activity android:name=".ViewPagerActivity" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".DialogActivity" />
        <activity
            android:name=".activity.toA_Activity"
            android:launchMode="standard" />
        <activity android:name=".activity.toB_Activity" />
        <activity android:name=".activity.toC_Activity" />
        <activity android:name=".activity.toD_Activity"></activity>
    </application>

</manifest>